{
  "name": "@nuintun/qrcode",
  "version": "3.1.1",
  "description": "A pure JavaScript QRCode encode and decode library.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuintun/qrcode.git"
  },
  "keywords": [
    "qrcode",
    "zxing"
  ],
  "sideEffects": false,
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuintun/qrcode/issues"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "clean": "node tools/clean.js",
    "prepublishOnly": "npm run build",
    "build:cjs": "rollup -c tools/rollup.cjs.js",
    "build:esm": "rollup -c tools/rollup.esm.js",
    "format": "prettier --write tools src examples",
    "build:examples": "rollup -c tools/rollup.examples.js",
    "build:main": "npm run build:cjs && npm run build:esm && npm run build:typings",
    "build:typings": "tsc --declaration --emitDeclarationOnly --declarationDir typings",
    "build": "npm run clean && npm run build:main && npm run build:examples && npm run format"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": "^2.67.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "magic-string": "^0.25.7",
    "@types/node": "^17.0.18",
    "@rollup/plugin-typescript": "^8.3.0",
    "@rollup/plugin-node-resolve": "^13.1.3"
  },
  "homepage": "https://github.com/nuintun/qrcode#readme"
}
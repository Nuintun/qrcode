{
  "name": "@nuintun/qrcode",
  "version": "4.0.0",
  "description": "A pure JavaScript QRCode encode and decode library.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuintun/qrcode.git"
  },
  "keywords": [
    "qrcode",
    "zxing"
  ],
  "sideEffects": false,
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuintun/qrcode/issues"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "clean": "node tools/clean.js",
    "prepublishOnly": "npm run build",
    "build:cjs": "rollup -c tools/rollup.cjs.js",
    "build:esm": "rollup -c tools/rollup.esm.js",
    "format": "prettier --write src tools examples",
    "examples": "rollup -c examples/rollup.config.js",
    "build": "npm run clean & npm run build:cjs & npm run build:esm && npm run examples && npm run format"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup": "^2.60.2",
    "prettier": "^2.5.1",
    "ts-toolbelt": "9.6.0",
    "typescript": "^4.5.2",
    "@types/node": "17.0.5",
    "magic-string": "^0.25.7",
    "@types/bluebird": "3.5.36",
    "rollup-plugin-typescript2": "^0.31.1",
    "@rollup/plugin-node-resolve": "^13.0.6"
  },
  "homepage": "https://github.com/nuintun/qrcode#readme"
}
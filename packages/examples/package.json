{
  "name": "webpack-antd-build",
  "description": "A webpack antd build tools.",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/nuintun/webpack-antd-builder",
  "nodemonConfig": {
    "quiet": true,
    "delay": "128ms",
    "watch": [
      ".swcrc.js",
      "tools/**/*",
      ".postcssrc.js",
      "app.config.js",
      "svgo.config.js",
      ".browserslistrc",
      "wwwroot/favicon.ico"
    ]
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "prod": "node tools/bin/webpack.config.prod.js",
    "dev": "nodemon tools/bin/webpack.config.dev.js",
    "format": "prettier --write . --ignore-path .prettierignore"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.1",
    "@nuintun/qrcode": "workspace:*",
    "@swc/helpers": "^0.5.6",
    "antd": "^5.15.2",
    "browser-fs-access": "^0.35.0",
    "copy-to-clipboard": "^3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13"
  },
  "devDependencies": {
    "@nuintun/svgo-loader": "^0.2.5",
    "@swc/core": "^1.4.6",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "autoprefixer": "^10.4.18",
    "bufferutil": "^4.0.8",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "6.0.0",
    "css-modules-types-loader": "^0.3.31",
    "find-free-ports": "^3.1.1",
    "html-webpack-plugin": "^5.6.0",
    "koa": "^2.15.0",
    "koa-compress": "^5.1.1",
    "memfs": "^4.7.7",
    "mini-css-extract-plugin": "^2.8.1",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "svgc-loader": "^0.2.5",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.4.2",
    "utf-8-validate": "^6.0.3",
    "webpack": "^5.90.3",
    "webpack-dev-service": "^0.6.1"
  }
}

{
  "name": "webpack-antd-build",
  "description": "A webpack antd build tools.",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/nuintun/webpack-antd-builder",
  "nodemonConfig": {
    "quiet": true,
    "delay": "128ms",
    "watch": [
      ".swcrc.js",
      "tools/**/*",
      ".postcssrc.js",
      "app.config.js",
      "svgo.config.js",
      ".browserslistrc",
      "wwwroot/favicon.ico"
    ]
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "prod": "node tools/bin/webpack.config.prod.js",
    "dev": "nodemon tools/bin/webpack.config.dev.js",
    "format": "prettier --write . --ignore-path .prettierignore"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@nuintun/qrcode": "workspace:*",
    "@swc/helpers": "^0.5.3",
    "antd": "^5.12.1",
    "browser-fs-access": "^0.35.0",
    "copy-to-clipboard": "^3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11"
  },
  "devDependencies": {
    "@nuintun/svgo-loader": "^0.2.3",
    "@swc/core": "^1.3.100",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "bufferutil": "^4.0.8",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "5.0.1",
    "css-modules-types-loader": "^0.3.29",
    "find-free-ports": "^3.1.1",
    "html-webpack-plugin": "^5.5.3",
    "koa": "^2.14.2",
    "koa-compress": "^5.1.1",
    "memfs": "^4.6.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "svgc-loader": "^0.2.2",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.3.2",
    "utf-8-validate": "^6.0.3",
    "webpack": "^5.89.0",
    "webpack-dev-service": "^0.5.18"
  }
}
